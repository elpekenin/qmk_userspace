
#
# userspace
#

#
# keylog
#
CONFIG_KEYLOG_ENABLE=y
CONFIG_KEYLOG_SIZE=40
# end of keylog

#
# quantum painter
#
CONFIG_QP_ASSETS_SIZE=30
CONFIG_QP_COMPUTER_STATS_SIZE=30

#
# logging
#
CONFIG_QP_LOG_ENABLE=y
CONFIG_QP_LOG_N_CHARS=70
CONFIG_QP_LOG_N_LINES=9
# end of logging
# end of quantum painter
# end of userspace

#
# modules
#

#
# allocator
#
CONFIG_ALLOC_ALLOCATORS_SIZE=10
CONFIG_ALLOC_ALLOCATIONS_SIZE=100
# end of allocator

#
# crash
#
CONFIG_CRASH_UNWIND_DEPTH=100
CONFIG_CRASH_MESAGE_LENGTH=200
# end of crash

#
# glitch text
#
CONFIG_GLITCH_TEXT_N_WORKERS=15
# end of glitch text

#
# logging
#
CONFIG_LOGGING_FORMAT="[%T] (%LS): %M"
# end of logging

#
# sanitizer
#
CONFIG_KASAN_INIT_DELAY=3000
CONFIG_KASAN_REDZONE_SIZE=4
CONFIG_KASAN_MALLOC_ARRAY_SIZE=300
# end of sanitizer

#
# scrolling text
#
CONFIG_SCROLLING_TEXT_N_WORKERS=15
# end of scrolling text
# end of modules
