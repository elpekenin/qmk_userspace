{
    "repo": "https://github.com/qmk/qmk_firmware",
    "branch": "xap",
    "path": "~/workspace/qmk",
    "default_compilation": false,
    "operations": [
        {
            "operation": "cp",
            "orig": "keyboards/elpekenin/access/*",
            "destination": "keyboards/elpekenin/access"
        },
        {
            "operation": "cp",
            "orig": "users/elpekenin/*",
            "destination": "users/elpekenin"
        },
        {
            "operation": "cp",
            "orig": "modules/*",
            "destination": "modules"
        },
        {
            // 3color eink
            "operation": "pr",
            "id": 20032
        },
        {
            // 22538-ish
            "operation": "merge",
            "repo": "https://github.com/elpekenin/qmk_firmware",
            "branch": "feature/rtt"
        },
        {
            // 24848-ish
            "operation": "merge",
            "repo": "https://github.com/elpekenin/qmk_firmware",
            "branch": "fix/modules"
        },
        {
            "operation": "checkout",
            "files": ["drivers/painter/il91874"],
            "repo": "https://github.com/elpekenin/qmk_firmware",
            "branch": "il91874"
        },
        {
            "operation": "diff",
            "file": "il91874.diff"
        },
        {
            "operation": "diff",
            "file": "spi_comms.diff"
        },
        {
            "operation": "diff",
            "file": "generic_rotation.diff"
        }
        // {
        //     // was this for 2nd core anims?
        //     "operation": "diff",
        //     "file": "animation.diff"
        // },
    ],
    "compile_command": "./keyboards/elpekenin/access/compile",
    "wsl": {
        "destination": "/mnt/d"
    }
}
