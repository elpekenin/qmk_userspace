{
    "repo": "https://github.com/qmk/qmk_firmware",
    "branch": "xap",
    "path": "$QMK_HOME",
    "operations": [
        {
            "display": "convert Python source to C strings",
            "operation": "exec",
            "cmd": "euc py2c keyboards/elpekenin/access/keymaps/elpekenin/py"
        },
        {
            "operation": "cp",
            "src": "keyboards/elpekenin/access",
            "dst": "$QMK_HOME/keyboards/elpekenin/access"
        },
        {
            "operation": "cp",
            "src": "users/elpekenin",
            "dst": "$QMK_HOME/users/elpekenin"
        },
        {
            "operation": "cp",
            "src": "modules",
            "dst": "$QMK_HOME/modules"
        },
        {
            "operation": "pr",
            "id": 20032
        },
        {
            "operation": "pr",
            "id": 25407
        },
        {
            "operation": "checkout",
            "files": ["drivers/painter/il91874"],
            "repo": "https://github.com/elpekenin/qmk_firmware",
            "branch": "il91874"
        },
        {
            "operation": "diff",
            "file": "patches/il91874.diff"
        },
        {
            "operation": "diff",
            "file": "patches/spi_comms.diff"
        },
        {
            "operation": "diff",
            "file": "patches/generic_rotation.diff"
        },
        {
            "display": "install branch deps",
            "operation": "exec",
            "cmd": "$(head -n 1 $(which qmk) | cut -c 3-) -m pip install -r requirements-dev.txt",
            "path": "$QMK_HOME"
        },
        {
            "display": "compiling",
            "operation": "exec",
            "cmd": "qmk --no-color compile -kb elpekenin/access -km elpekenin ${COMPILEDB---compiledb}",
            "path": "$QMK_HOME"
        }
    ]
}
